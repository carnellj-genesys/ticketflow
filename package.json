{
  "name": "ticketflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node server/server.js",
    "dev:client": "vite",
    "server": "node server/server.js",
    "dev:server-compatible": "node server/server-compatible.js",
    "server-compatible": "node server/server-compatible.js",
    "dev:debug": "LOG_LEVEL=DEBUG node server/server.js",
    "dev:trace": "LOG_LEVEL=TRACE node server/server.js",
    "dev:file": "LOG_LEVEL=DEBUG LOG_FILE=true node server/server.js",
    "dev:performance": "LOG_LEVEL=INFO LOG_PERFORMANCE=true node server/server.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "better-sqlite3": "^12.2.0",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "json-server": "^0.17.4",
    "msw": "^2.0.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "config": {
    "sqlite3": {
      "binary": {
        "host": "https://github.com/mapbox/node-sqlite3/releases/download/",
        "remote_path": "{version}/node-v{napi_version}-{platform}-{arch}.tar.gz"
      }
    }
  }
}
